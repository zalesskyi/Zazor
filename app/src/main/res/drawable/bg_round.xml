<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="8dp"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp" />
            <stroke
                android:width="1dp"
                android:color="@color/colorAccent_20" />
        </shape>
        <bitmap android:src="@drawable/ic_flip" />
    </item>
</layer-list>